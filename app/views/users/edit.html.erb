<%= form_with model: @user do |form| %>
  <%= form.label "Your user name here" %>
  <%= form.text_field :username %>
  <%= form.label :email %>
  <%= form.email_field :email, placeholder: 'example@example.com' %>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <%= form.submit "Update" %>
<% end %>